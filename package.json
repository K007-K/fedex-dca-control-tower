{
  "name": "fedex-dca-control-tower",
  "version": "0.1.0",
  "private": true,
  "description": "Enterprise-grade Debt Collection Agency Management System for FedEx",
  "scripts": {
    "dev": "pnpm --filter web dev",
    "build": "pnpm --filter web build",
    "start": "pnpm --filter web start",
    "lint": "pnpm --filter web lint",
